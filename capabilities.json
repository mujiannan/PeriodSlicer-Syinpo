    "suppressDefaultTitle": true,
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "Dates": {
                        "max": 1,
                        "min": 1
                    },
                    "StartDate": {
                        "max": 1,
                        "min": 0
                    },
                    "EndDate": {
                        "max": 1,
                        "min": 0
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "Dates"
                    },
                    "dataReductionAlgorithm": {
                        "top": {
                            "count": 1
                        }
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "StartDate"
                            }
                        },
                        {
                            "bind": {
                                "to": "EndDate"
                            }
                        }
                    ]
                }
            }
        }
    ]
