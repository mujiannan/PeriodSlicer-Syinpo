                            },
                            {
                                "value": "3",
                                "displayName": "Wednesday",
                                "displayNameKey": "Wednesday"
                            },
                            {
                                "value": "4",
                                "displayName": "Thursday",
                                "displayNameKey": "Thursday"
                            },
                            {
                                "value": "5",
                                "displayName": "Friday",
                                "displayNameKey": "Friday"
                            },
                            {
                                "value": "6",
                                "displayName": "Saturday",
                                "displayNameKey": "Saturday"
                            }
                        ]
